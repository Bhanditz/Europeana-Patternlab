{{> atoms/iconography/svgicons-pro}}

{{> organisms/global/header}}

{{> organisms/components/site-hero }}


<!-- Start Main page content -->
<div  id="maincontent" role="main" class="labs-home">


{{> organisms/sections/strapline }}

{{> organisms/sections/news }}

{{> organisms/sections/quickinfo }}


<section class="section events-upcoming">
	<div class="lc">

		<h2 class="section-title">Upcoming Events</h2>

		<div class="grid cf">
		{{#listItems.two}}{{> molecules-event-block-small}}{{/listItems.two}}
		</div>

		<a class="link-more" href="events">More Events</a>

	</div>
</section>




{{! Apps list }}
<section class="apps-list section">
	<div class="lc">

	<h2>Be inspired …</h2>

	<ul class="l-post-list">
	    {{#listItems.nine}}<li>
            <div class="block block-thumb">

                <div class="b-thumb">
                    <a href="{{url}}">
                        <img src="{{image_root}}{{img.rectangle.src}}" alt="{{image.rectangle.alt}}">
                    </a>
                </div>

                <div class="b-text">
	                    <h2 class="headline"><a href="{{url}}">{{headline.short}}</a></h2>
	                    <div class="description">
	                        <p>{{excerpt.medium}}</p>
	                    </div>
					
					<a href="{{url}}" title="Europeana's pilot on Google's Field Trip app">Find out more</a>
                </div>
            </div>
        </li>{{/listItems.nine}}
	</ul>


	<div class="apps-more">
		<p><a href="/apps" title="Application Gallery"><strong>See more examples in the Application Gallery&nbsp;→</strong></a></p>
	</div>


	</div>
</section>

</div>

{{> organisms/global/footer }}