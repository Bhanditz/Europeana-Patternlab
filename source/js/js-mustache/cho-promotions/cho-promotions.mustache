                  <div class="promo-title-wrap">
                    <div class="promo-title[[#icon]] with-icon[[/icon]][[#tags]] has-tags-after[[/tags]][[#relation]] has-relation-after[[/relation]]">
                      [[#url]]<a href="[[.]]">[[/url]][[.]][[#url]]</a>[[/url]]
                    </div>
                    [[#icon]]
                        <span class="icon icon-[[.]]"></span>
                    [[/icon]]
                  </div>
          
                  [[#tags]]
                    <ul class="promo-tags">
                      [[#items]]
                        <li>
                          [[#url]]<a class="promo-tag-link" href="[[.]]">[[/url]]#[[text]][[#url]]</a>[[/url]]
                        </li>
                      [[/items]]
                    </ul>
                  [[/tags]]
          
                </div>
              [[/title]]
          
              [[#excerpt]]
                [[#short]]
                  <div class="text-main[[^relation]] no-relation[[/relation]]">[[[ . ]]]</div>
                [[/short]]
