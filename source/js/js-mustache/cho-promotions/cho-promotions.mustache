                      [[#img]]
                        <div class="channel-object-viewmore">
                          <span class="viewmore-image" style="background-image: url('[[.]]');"></span>
                        </div>
                      [[/img]]
                      [[#title]]
                         <span class="title">
                           [[.]]
                         </span>
                      [[/title]]
                      [[#description]]
                        [[.]]
                      [[/description]]
                      <span class="links">
                        [[#link_related]]
                          [[#url]]
                            <a href="[[.]]">[[text]]</a>
                          [[/url]]
                        [[/link_related]]
                        [[#link_more]]
                          [[#url]]
                            <a href="[[.]]">[[text]]</a>
                          [[/url]]
                        [[/link_more]]
